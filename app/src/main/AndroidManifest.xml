<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naien.workout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_workout"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/OrangeApp.ActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity
            android:name=".NewWorkoutActivity">
            android:theme="@style/OrangeApp.NoActionBar">
        </activity>
        <activity
            android:name=".WorkoutMainActivity"
            android:theme="@style/green.ActionBar">
        </activity>
        <activity
            android:name=".ExerciseMainActivity">
            android:theme="@style/OrangeApp.NoActionBar">
        </activity>
        <activity
            android:name=".WorkoutMainStaticActivity"
            android:theme="@style/purple.ActionBar">
        </activity>
        <activity
            android:name=".ExerciseMainStaticActivity">
            android:theme="@style/OrangeApp.NoActionBar">
        </activity>

    </application>

</manifest>
